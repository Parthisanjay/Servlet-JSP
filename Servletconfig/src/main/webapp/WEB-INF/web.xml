<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>servlet1</servlet-name>
  	<servlet-class>com.servletconfig.Servlet1</servlet-class>
  	
  	<init-param>
  		<param-name>email</param-name>
